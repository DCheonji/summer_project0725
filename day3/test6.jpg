<!--
//메뉴 스타일 불러옴 : 메뉴 /script/menubar.js 와 같이 사용되어야 함
//submenuStyle	: 서브메뉴 펼침 스타일 (horizon : 가로로, vertical : 세로 슬라이딩)
var menuIndexHold = true,
menuOverHold = true,
submenuStyle = "vertical",
sizeMode = "basic",
subShow = "hidden",
submenuPosi = "center",
effectSwitch = "on",
subSlideSpeed = 150,
subFadeSpeed = 150;

$(document).ready(function(){
	// all menu
	var allSections = $('#all-sections')
	var allSecBtn = $('.user-allbtn');

	allSecBtn.on("click", function(e){
		e.preventDefault();
		if(allSections.css("display") == "none"){
			allSecBtn.addClass('on');
			if(subFadeSpeed > 0) {
				allSections.slideDown(subSlideSpeed);
			} else {
				allSections.show();
			}
		} else {
			allSecBtn.removeClass('on');
			if(subFadeSpeed > 0) {
				allSections.slideUp(subSlideSpeed);
			} else {
				allSections.hide();
			}
		}
	});

	// search
	var schBox = $('.user-search')
		, schBtns = schBox.find('.sch-btns')
		, schForm = schBox.find('.user-search-form');

	schBtns.on({
		click: function(){
			schForm.show().find('input#search').focus();
		}
	});

	$(document).mouseup(function(e) {	
		if (!schBox.is(e.target) && schBox.has(e.target).length === 0) {
			schForm.hide();
		}

		if (!allSections.is(e.target) && allSections.has(e.target).length === 0) {
			allSecBtn.removeClass('on');
			allSections.slideUp();
		}
	});
});
</script>

