	<script>
	$(".hk-family-select select").on("change", function(){
		url = $(this).val()

		if(!url){
			return;
		}

		let target = $(this).find("option:selected").data("target");
		// 새창열기
		if(target == "_blank"){
			window.open(url, "_blank");
		}
		// 인창열기
		else{
			location.href = url;
		}
	});
	</script>
