<script type="module">
let retry = false;

$(document).ready(() => {
    // AI 추천 뉴스 오픈
    $('.btn-open-recommended').on("click", function() {
        $(this).removeClass('floating-slide-in').addClass('floating-slide-out');

        $(".layer-floating-recommended .loading").show();
        $(".layer-floating-recommended .recommended-news-list").hide();

        $('.layer-floating-recommended').removeClass('floating-slide-out').addClass('floating-slide-in');

        getAIRecommentNews();
    });

    // AI 추천 뉴스 닫기
    $('.floating-btn-close').on("click", function() {
        $(this).parents('.layer-floating-recommended').removeClass('floating-slide-in').addClass('floating-slide-out');
        $('.btn-open-recommended').removeClass('floating-slide-out').addClass('floating-slide-in');
    });

    $(document).on('mouseup',function(e){
        if(
            $(e.target).hasClass('layer-floating-recommended') == false
            && $(e.target).parents().hasClass('layer-floating-recommended') == false
        ) {
            $('.layer-floating-recommended').removeClass('floating-slide-in');
            $('.btn-open-recommended').removeClass('floating-slide-out');
        };
    });
});

const getAIRecommentNews = () => {
    $.ajax({
        url: '/api/article/ai-personal-recommend-article'
        , success: (result) => {
            try{
                // 2024.12.23 리스트 데이터 없으면 에러 메시지 노출
                if(result.list.length == 0){
                    throw new Error("no list of recommend list"); 
                }

                $(".layer-floating-recommended ul.recommended-news-list").html('');
                $(".layer-floating-recommended ul.recommended-news-list").data("attributionToken", result.attributionToken);

                $.each(result.list, (idx, val) => {
                    $(".layer-floating-recommended ul.recommended-news-list").append(`
                        <li>
                            <div class="news-item">
                                <div class="txt-cont">
                                    <a href="${val.url}" class="news-tit">${val.title}</a>
                                </div>
                                ${val.thumbimg ? 
                                `<div class="thumb">
                                    <a href="${val.url}">
                                        <img src="${val.thumbimg}" alt="${htmlspecialchars(val.title)}">
                                    </a>
                                </div>` 
                                : ''
                                }
                            </div>
                        </li>
                    `);
                });

                $(".layer-floating-recommended .loading").hide();
                $(".layer-floating-recommended .recommended-news-list").show();
            }catch(e){
                if(retry != true){
                    retry = true;
                    getAIRecommentNews();
                }
            }
        }
        , error: (xhr, status, responseTxt) => {
            console.log(xhr);
            alert("AI 추천뉴스 호출 도중 에러가 발생했습니다.");

            $('.floating-btn-close').parents('.layer-floating-recommended').removeClass('floating-slide-in').addClass('floating-slide-out');
            $('.btn-open-recommended').removeClass('floating-slide-out').addClass('floating-slide-in');
        }
    });
}
</script>


